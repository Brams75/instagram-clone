{
  "name": "instagram-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "yarn check-format && yarn check-lint && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --browser chrome --headless",
    "test:e2e": "start-server-and-test 'yarn start' 3000 'yarn cypress'",
    "test:e2e:ci": "start-server-and-test 'yarn start' 3000 'yarn cypress:headless'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.16.8",
    "cypress": "^8.0.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "jest": "^27.0.6",
    "next": "^12.1.0",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "start-server-and-test": "^1.13.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-testing-library": "^4.10.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
